
@{
    ViewBag.Title = "One";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container" id="layoutEmpty_container">
    <div class="col-xs-12 col-sm-10 col-sm-offset-2 col-md-8 col-md-offset-2 col-lg-6 col-md-offset-3">
        <div class="card">
            <div class="card-header bg-info text-md-center" style="color:white;">
                <strong> ATM </strong>
            </div>
            <div Class="card-block">
                <div Class="form-group row">
                    <Label for="txtAmount" class="col-md-4 text-md-right">Enter Amount:</Label>
                    <div Class="col-md-8">
                        <input class="form-control" id="txtAmount" name="txtAmount" type="text" value="" />
                    </div>
                </div>
                <div Class="form-group row text-md-center">
                    <label id="error_msg" Class="text-danger col-md-12">
                    </label>
                </div>
            </div>
            <div Class="card-footer text-md-center">
                <input type="button" class="btn btn-success" id="btnDispense" value="Dispense amount" />
            </div>
        </div>
    </div>

    <div class="col-xs-12 col-sm-10 col-sm-offset-2 col-md-8 col-md-offset-2 col-lg-6 col-md-offset-3" id="outputTab" style="display:none">
        <div class="card">
            <div class="card-header bg-info text-md-center" style="color:white;">
                <strong> OUTPUT </strong>
            </div>
            <div Class="card-block">
                <div Class="form-group row">
                    <Label for="note500" class="col-md-4 text-md-right">Number of 500:</Label>
                    <div Class="col-md-8">
                        <input class="form-control" id="note500" name="note500" type="text" value="" />
                    </div>
                </div>
                <div Class="form-group row">
                    <Label for="note100" class="col-md-4 text-md-right">Number of 100:</Label>
                    <div Class="col-md-8">
                        <input class="form-control" id="note100" name="note100" type="text" value="" />
                    </div>
                </div>
                <div Class="form-group row">
                    <Label for="note50" class="col-md-4 text-md-right">Number of 50:</Label>
                    <div Class="col-md-8">
                        <input class="form-control" id="note50" name="note50" type="text" value="" />
                    </div>
                </div>
                <div Class="form-group row">
                    <Label for="note10" class="col-md-4 text-md-right">Number of 10:</Label>
                    <div Class="col-md-8">
                        <input class="form-control" id="note10" name="note10" type="text" value="" />
                    </div>
                </div>
                <div Class="form-group row">
                    <Label for="note5" class="col-md-4 text-md-right">Number of 5:</Label>
                    <div Class="col-md-8">
                        <input class="form-control" id="note5" name="note5" type="text" value="" />
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section scripts {
    <script src="~/JS/One.js"></script>
}


